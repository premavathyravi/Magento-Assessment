<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route method="GET" url="/V1/trackingcart/getlist">
        <service class="Tracking\Cart\Api\FetchAllCartDetailsInterface" method="getCartList"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="GET" url="/V1/trackingcart/getid">
        <service class="Tracking\Cart\Api\GetOneCartDetailsInterface" method="getCartById"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="POST" url="/V1/trackingcart/save">
        <service class="Tracking\Cart\Api\SaveCartDetailsInterface" method="saveCart"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="POST" url="/V1/trackingcart/update">
        <service class="Tracking\Cart\Api\UpdateCartDetailsInterface" method="updateCart"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="POST" url="/V1/trackingcart/delete">
        <service class="Tracking\Cart\Api\DeleteCartDetailsInterface" method="deleteCartById"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>
